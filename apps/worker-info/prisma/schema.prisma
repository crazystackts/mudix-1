datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Holder {
  addresses              Json?
  affectionateFirstName  String?
  affectionateGender     String?
  affectionateLastName   String?
  attachments            Json?
  birthDate              DateTime?
  contactAuthorizationId String?
  contacts               Json?
  documents              Json?
  editedFields           Json?
  education              String?
  filial                 Json?
  firstName              String?
  gender                 String?
  id                     String    @id @default(uuid())
  lastName               String?
  maritalStatus          String?
  password               String
  phones                 Json?
  requestId              String
  socialFirstName        String?
  socialGender           String?
  socialLastName         String?
  socioeconomicInfo      Json?
  token                  String?
  typeField              String?   @map("type")
  username               String    @unique
}

model File {
  attachmentsId String
  filename      String?
  id            String  @id @default(uuid())
  name          String?
  url           String?

  @@map("Files")
}

model Dependent {
  affectionateFirstName           String?
  affectionateLastName            String?
  birthDate                       DateTime?
  complete                        Boolean?
  credential                      String?
  dependentAddresses              Json?
  dependentAttachments            Json?
  dependentContactAuthorizationId String?
  dependentContacts               Json?
  dependentDocuments              Json?
  dependentPhones                 Json?
  education                       String?
  exclude                         Boolean?
  firstName                       String?
  gender                          String?
  id                              String    @id @default(uuid())
  lastName                        String?
  maritalStatus                   String?
  relationship                    String?
  requestId                       String
  socialFirstName                 String?
  socialLastName                  String?

  @@map("Dependents")
}
